{
  "name": "funasia-frontend",
  "description": "Frontend Game",
  "version": "0.0.2",
  "license": "MIT",
  "engines": {
    "npm": ">=6.12.0",
    "node": ">=12.13.0"
  },
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "FunAsia",
    "FunArcade",
    "Games"
  ],
  "scripts": {
    "// ---DEFAULT CRA---": "",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:origin": "react-app-rewired test",
    "// ---TEMPLATE---": "Scripts will be also in the template",
    "start:local": "env-cmd -f ./environments/.env.local yarn start:origin",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "eslint": "eslint --ext js,jsx,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src --max-warnings=0",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettier:fix": "prettier --write .",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "// ---TESTING TEMPLATE---": "",
    "test:internals": "yarn run test:extract-messages && yarn run test:generators",
    "test:extract-messages": "jest --config=internals/extractMessages/jest.config.js",
    "verify-startingTemplate-changes": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/verify-startingTemplate-changes.ts",
    "test:coverage": "yarn run test --watchAll=false --coverage",
    "test:cra": "yarn run create:cra-app && cd generated-cra-app && yarn run test:generators && yarn run lint && yarn run cleanAndSetup && yarn run lint",
    "// ---BUILDING TEMPLATE---": "",
    "create:npm-package": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-npm-package.script.ts",
    "create:cra-app": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-cra-app.script.ts",
    "// ---PUBLISHING---": "",
    "publish:github": "cross-env PUBLIC_URL='/react-boilerplate-cra-template' yarn run build && gh-pages -d build",
    "changelog": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-changelog.script.ts",
    "release": "standard-version",
    "publish:npm": "yarn run create:npm-package && yarn publish .cra-template-rb --non-interactive",
    "prepare": "npx husky install"
  },
  "lint-staged": {
    "!(*test|*bundle).{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings=1"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/reportWebVitals.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@hookform/resolvers": "2.9.0",
    "@mui/material": "5.2.6",
    "@mui/x-date-pickers": "5.0.12",
    "@reduxjs/toolkit": "1.7.1",
    "axios": "0.24.0",
    "axios-retry": "3.3.1",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.7",
    "echarts": "5.4.1",
    "echarts-for-react": "3.0.2",
    "ethers": "5.6.4",
    "html-react-parser": "3.0.1",
    "i18next": "21.6.4",
    "i18next-browser-languagedetector": "6.1.2",
    "jwt-decode": "3.1.2",
    "moment": "2.29.4",
    "react": "17.0.2",
    "react-app-polyfill": "3.0.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "7.31.3",
    "react-i18next": "11.16.6",
    "react-redux": "7.2.4",
    "react-router-dom": "6.2.1",
    "react-swipeable": "6.1.2",
    "react-toastify": "8.1.0",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "13.0.0",
    "styled-components": "5.3.3",
    "swiper": "8.1.3",
    "typesafe-actions": "5.1.0",
    "utf8": "3.0.0",
    "web-vitals": "1.1.2",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/react": "18.0.5",
    "@types/react-dom": "17.0.14",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.9",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.0",
    "buffer": "6.0.3",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.1.0",
    "husky": "7.0.4",
    "i18next-scanner": "3.0.0",
    "inquirer": "8.0.0",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.4",
    "lint-staged": "12.1.4",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "prettier": "2.5.1",
    "react-app-rewired": "2.2.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "standard-version": "9.2.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "typescript": "4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
